server.port=<%= @app_port %>

#--- Spring Admin ---#
spring.boot.admin.url=http://localhost:8089
#--- Spring Admin ---#

#--- Security ---#
#security.user.name=admin
#security.user.password=admin
#security.user.role=DASHBOARD_ADMIN
security.url=http://dashboard.metrixware.local:8096
#--- Security ---#

#--- ActiveMQ ---# https://github.com/spring-projects/spring-boot/blob/v1.2.5.RELEASE/spring-boot-autoconfigure/src/main/java/org/springframework/boot/autoconfigure/jms/activemq/ActiveMQProperties.java
spring.activemq.broker-url=tcp://127.0.0.1:61616?jms.redeliveryPolicy.maximumRedeliveries=-1&jms.redeliveryPolicy.redeliveryDelay=60000
#spring.activemq.broker-url=vm://localhost?jms.redeliveryPolicy.maximumRedeliveries=-1&jms.redeliveryPolicy.redeliveryDelay=60000
spring.activemq.user=<%= @default_user %>
spring.activemq.password=<%= @default_password %>
#spring.activemq.client-id=timeseries
#spring.activemq.in-memory=true
#spring.activemq.pooled=false
#--- ActiveMQ ---#

#--- Elasticsearch ---#
elasticsearch.cluster=tocea
elasticsearch.node=komea
#elasticsearch.local=true
elasticsearch.local=false
elasticsearch.path-home=build/elasticsearch-home
elasticsearch.host=127.0.0.1
elasticsearch.port=9300
#--- Elasticsearch ---#

#------ Logging ------#
logging.config=logback.xml
#------ Logging ------#
